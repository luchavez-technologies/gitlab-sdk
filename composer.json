{
    "name": "luchavez/gitlab-sdk",
    "description": "Gitlab REST API Laravel-based SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarloluchavez@gmail.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez/"
        },
        {
            "name": "Fligno",
            "email": "apps@fligno.com",
            "homepage": "https://fligno.com/"
        }
    ],
    "homepage": "https://github.com/luchmewep/gitlab-sdk",
    "keywords": ["Laravel", "GitlabSdk"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~8|~9",
        "luchavez/api-sdk-kit": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~6|~7"
    },
    "autoload": {
        "psr-4": {
            "Luchavez\\GitlabSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchavez\\GitlabSdk\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchavez\\GitlabSdk\\Providers\\GitlabSdkServiceProvider"
            ],
            "aliases": {
                "GitlabSdk": "Luchavez\\GitlabSdk\\Facades\\GitlabSdk"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
